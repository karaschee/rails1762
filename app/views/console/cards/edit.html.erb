<p class="lead">
  编辑资料卡 - <%= @card.title %>
</p>
<%= render partial: 'console/shared/errors', locals: { object: @card } %>
<%= form_for @card, url: console_card_path(@card), method: :patch do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <div class="form-group">
    <button type="submit" class="btn btn-primary">编辑</button>
  </div>
<% end %>