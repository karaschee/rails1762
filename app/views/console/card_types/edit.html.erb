<p class="lead">
  编辑资料卡分类 - <%= @card_type.name %>
</p>
<%= render partial: 'console/shared/errors', locals: { object: @card_type } %>
<%= form_for @card_type, url: console_card_type_path(@card_type), method: :patch do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <div class="form-group">
    <button type="submit" class="btn btn-primary">编辑</button>
  </div>
<% end %>